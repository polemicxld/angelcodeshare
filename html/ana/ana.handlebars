      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
    <link
      rel="icon"
      href="https://cdn.discordapp.com/attachments/837256100067409940/849575899560017920/a_1e94cf8a957c16068cfd2bbcf4c70a1a.png"
      type="image/x-icon"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl"
      crossorigin="anonymous"
    />
    <script
      src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"
    ></script>
    <script
      src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"
    ></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/clipboard.js/2.0.10/clipboard.min.js"></script>
    <script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-2783231188593099"
     crossorigin="anonymous"></script>
    <title>Angel Code Share</title>
  </head>

  <style>
    .bd-placeholder-img { font-size: 1.125rem; text-anchor: middle;
    -webkit-user-select: none; -moz-user-select: none; user-select: none; }
    @media (min-width: 768px) { .bd-placeholder-img-lg { font-size: 3.5rem; } }
    body { background-color:#363940; color: white}
  </style>

  <!-- Custom styles for this template -->
  <link href="/sticky-footer-navbar.css" rel="stylesheet" />
  <body class="d-flex flex-column h-100">
    <nav
      class="navbar navbar-expand-lg navbar-dark bg-dark"
      style="background-color:#141515"
    >
      <div class="container-fluid">
        <a class="navbar-brand" href="/">Angel Code Share</a>
        <button
          class="navbar-toggler border-0 p-0"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarSupportedContent"
          aria-controls="navbarSupportedContent"
          aria-expanded="false"
          aria-label="Toggle navigation"
          style="box-shadow:none"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">
            <li class="nav-item">
              <a class="nav-link" href="/">Anasayfa</a>
            </li>
            <li class="nav-item dropdown">
              <a
                class="nav-link dropdown-toggle"
                href="#"
                id="navbarDropdown"
                role="button"
                data-bs-toggle="dropdown"
                aria-expanded="false"
              >
                Kodlar
              </a>
              <ul
                class="dropdown-menu dropdown-menu-dark"
                aria-labelledby="navbarDropdown"
              >
                <li><a class="dropdown-item" href="/bdfd"><i
                      class="fas fa-robot"
                    ></i>
                    BDFD</a></li>
                <li><a class="dropdown-item" href="/aoijs"><i
                      class="fab fa-node-js"
                    ></i>
                    AOI.js</a></li>
                <li><a class="dropdown-item" href="/discordjs"><i
                      class="fab fa-js"
                    ></i>
                    Discord.js</a></li>
                <li><a class="dropdown-item" href="/html"><i
                      class="fab fa-html5"
                    ></i>
                    HTML</a></li>
                <li><a class="dropdown-item" href="/altyapi"><i
                      class="fas fa-file-code"
                    ></i>
                    Altyapı</a></li>
              </ul>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/tavsiye-ver">Tavsiye</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/dc">Discord</a>
            </li>
          </ul>
          {{#if user}}
            <div class="dropdown">
              <button
                class="btn btn-light dropdown-toggle"
                style="box-shadow:none"
                type="button"
                id="dropdownMenuButton1"
                data-bs-toggle="dropdown"
                aria-expanded="false"
              >
                <i class="bi bi-person-fill"></i>
                {{user.username}}
              </button>
              <ul
                class="dropdown-menu dropdown-menu-darj"
                aria-labelledby="dropdownMenuButton1"
              >
                <li><a class="dropdown-item" href="/kod-panel"><i
                      class="fas fa-code"
                    ></i>
                    Kod Paneli</a></li>
                <li><a class="dropdown-item" href="/quit"><i
                      class="fas fa-sign-out-alt"
                    ></i>
                    Çıkış Yap</a></li>
              </ul>
            </div>
          {{else}}
            <a href="/join" style="box-shadow:none" class="btn btn-success"><i
                class="fas fa-sign-in-alt"
              ></i>Giriş Yap</a>
          {{/if}}
        </div>
      </div>
    </nav>
    {{{body}}}
    <footer class="pt-4 my-md-5 pt-md-5 border-top"><center>
        <p>© Sitedeki Kodların Çalınması Yasaktır.</p>
        <p>Coded by
          <a
            href="https://discord.gg/eN4WbfymCv"
            style="color:#77DD77; text-decoration:none"
          >FurkanCanKar</a></p>
      </center></footer><br />

    <script
      data-ad-client="ca-pub-2783231188593099"
      async
      src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"
    ></script>

    <!-- Option 1: Bootstrap Bundle with Popper -->
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-b5kHyXgcpbZJO/tY9Ul7kGkf1S0CWuKcCD38l8YkeH8z8QjE0GmW1gYU5S9FOnJ0"
    ></script>
    <!-- Option 2: Separate Popper and Bootstrap JS -->
